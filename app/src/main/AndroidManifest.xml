<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="kr.ive.interstitial_sdk_sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data android:name="ive_sdk_appcode" android:value="TIdKKXBq9C" />
        <meta-data android:name="ive_interstitial_sdk_join_button_color" android:resource="@android:color/white" />
        <meta-data android:name="ive_interstitial_sdk_join_button_text_color" android:resource="@android:color/black" />
    </application>

</manifest>